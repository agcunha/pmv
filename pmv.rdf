<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
    xmlns:pmv="http://purl.org/pmv/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:spin="http://spinrdf.org/spin#"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:wot="http://xmlns.com/wot/0.1/"
    xmlns:org="http://www.w3.org/ns/org#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://purl.org/pmv/">
  <owl:Ontology rdf:about="">
    <dc:creator>https://www.linkedin.com/in/agcunha/</dc:creator>
    <dc:title xml:lang="en-us">Process Modeling Vocabulary</dc:title>
    <dc:description xml:lang="en-us">Allow detailed modeling of process flows by adding events, products, and controls to the Enterprise Architecture</dc:description>
    <owl:versionInfo>Process Modeling Vocabulary v0.1 </owl:versionInfo>
    <dc:title xml:lang="pt-br">Vocabulário de Processos</dc:title>
    <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
    <dc:description xml:lang="pt-br">Modelagem de fluxo de processos</dc:description>
    <dc:identifier>pmv</dc:identifier>
    <dc:date>2017-07</dc:date>
  </owl:Ontology>
  <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
  <owl:Class rdf:about="http://purl.org/pmv/Event">
    <dc:description xml:lang="pt-br">Uma alteração de estado organizacional que aciona eventos de processamento; Podem ser originados dentro ou fora da organização e podem ser resolvidos dentro ou fora da organização.</dc:description>
    <dc:description xml:lang="en-us">An organizational state change that triggers processing events; may originate from inside or outside the organization and may be resolved inside or outside the organization.</dc:description>
    <rdfs:label xml:lang="en-us">Event</rdfs:label>
    <dc:title xml:lang="pt-br">Evento</dc:title>
    <rdfs:label xml:lang="pt-br">Evento</rdfs:label>
    <rdfs:comment xml:lang="en-us">An organizational state change that triggers processing events; may originate from inside or outside the organization and may be resolved inside or outside the organization.</rdfs:comment>
    <rdfs:comment xml:lang="pt-br">Uma alteração de estado organizacional que aciona eventos de processamento; Podem ser originados dentro ou fora da organização e podem ser resolvidos dentro ou fora da organização.</rdfs:comment>
    <dc:title xml:lang="en-us">Event</dc:title>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/pmv/Product">
    <dc:title xml:lang="pt-br">Produto</dc:title>
    <dc:title xml:lang="en-us">Product</dc:title>
    <dc:description xml:lang="pt-br">Saída gerada pelo negócio. Um produto do negócio resultante da execução de um processo</dc:description>
    <dc:description xml:lang="en-us">Output generated by the business. The business product of the execution of a process.</dc:description>
    <rdfs:label xml:lang="en-us">Product</rdfs:label>
    <rdfs:label xml:lang="pt-br">Produto</rdfs:label>
    <rdfs:comment xml:lang="en-us">Output generated by the business. The business product of the execution of a process.</rdfs:comment>
    <rdfs:comment xml:lang="pt-br">Saída gerada pelo negócio. Um produto do negócio resultante da execução de um processo</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/pmv/Function">
    <dc:description xml:lang="pt-br">Fornece capacidades empresariais estreitamente alinhadas a uma organização, mas não necessariamente explicitamente governadas pela organização. Também referida como "função comercial".</dc:description>
    <dc:description xml:lang="en-us">Delivers business capabilities closely aligned to an organization, but not necessarily explicitly governed by the organization. Also referred to as "business function".</dc:description>
    <dc:title xml:lang="pt-br">Função</dc:title>
    <dc:title xml:lang="en-us">Function</dc:title>
    <rdfs:comment xml:lang="en-us">Delivers business capabilities closely aligned to an organization, but not necessarily explicitly governed by the organization. Also referred to as "business function".</rdfs:comment>
    <rdfs:label xml:lang="pt-br">Função</rdfs:label>
    <rdfs:comment xml:lang="pt-br">Fornece capacidades empresariais estreitamente alinhadas a uma organização, mas não necessariamente explicitamente governadas pela organização. Também referida como "função comercial".</rdfs:comment>
    <rdfs:label xml:lang="en-us">Function</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/pmv/Business_Service">
    <rdfs:label xml:lang="en-us">Business Service</rdfs:label>
    <dc:title xml:lang="en-us">Business Service</dc:title>
    <rdfs:comment xml:lang="pt-br">Suporta capacidades empresariais através de uma interface explicitamente definida e é explicitamente governada por uma organização.</rdfs:comment>
    <dc:title xml:lang="pt-br">Serviço de Negócio</dc:title>
    <rdfs:comment xml:lang="en-us">An element of behavior that provides specific functionality in response to requests from actors or other services. A service delivers or supports business capabilities, has an explicitly defined interface, and is explicitly governed. Services are defined for business, information systems, and platforms.</rdfs:comment>
    <dc:description xml:lang="pt-br">Suporta capacidades empresariais através de uma interface explicitamente definida e é explicitamente governada por uma organização.</dc:description>
    <rdfs:label xml:lang="pt-br">Serviço de Negócio</rdfs:label>
    <dc:description xml:lang="en-us">An element of behavior that provides specific functionality in response to requests from actors or other services. A service delivers or supports business capabilities, has an explicitly defined interface, and is explicitly governed. Services are defined for business, information systems, and platforms.</dc:description>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent">
    <rdfs:label xml:lang="pt-br">Ator</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en-us">Agent</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/org#Organization">
    <rdfs:label xml:lang="en-us">Organization</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="pt-br">Organização</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/pmv/Process">
    <dc:description xml:lang="pt-br">Um processo representa o fluxo de controle entre ou dentro de funções e / ou serviços (depende da granularidade da definição).&#xD;
Os processos representam uma seqüência de atividades que juntos conseguem um resultado especificado, podem ser decompostas em subprocessos e podem mostrar a operação de uma função ou serviço (no próximo nível de detalhe). Os processos também podem ser usados para vincular ou compor organizações, funções, serviços e processos.</dc:description>
    <dc:description xml:lang="en-us">A process represents flow of control between or within functions and/or services (depends on the granularity of definition).&#xD;
Processes represent a sequence of activities that together achieve a specified outcome, can be decomposed into sub-processes, and can show operation of a function or service (at next level of detail). Processes may also be used to link or compose organizations, functions, services, and processes.</dc:description>
    <rdfs:label xml:lang="en-us">Process</rdfs:label>
    <rdfs:label xml:lang="pt-br">Processo</rdfs:label>
    <rdfs:comment xml:lang="pt-br">Um processo representa o fluxo de controle entre ou dentro de funções e / ou serviços (depende da granularidade da definição).
Os processos representam uma seqüência de atividades que juntos conseguem um resultado especificado, podem ser decompostas em subprocessos e podem mostrar a operação de uma função ou serviço (no próximo nível de detalhe). Os processos também podem ser usados para vincular ou compor organizações, funções, serviços e processos.</rdfs:comment>
    <rdfs:comment xml:lang="en-us">A process represents flow of control between or within functions and/or services (depends on the granularity of definition).
Processes represent a sequence of activities that together achieve a specified outcome, can be decomposed into sub-processes, and can show operation of a function or service (at next level of detail). Processes may also be used to link or compose organizations, functions, services, and processes.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/pmv/Control">
    <rdfs:comment xml:lang="pt-br">Uma etapa de tomada de decisão com lógica de decisão acompanhante utilizada para determinar a abordagem de execução de um processo ou para garantir que um processo esteja em conformidade com critérios de governança. Por exemplo, um controle de aprovação no processo de processamento de solicitação de compra que verifica se o valor total do pedido está dentro dos limites de assinatura do solicitante ou se ele precisa escalar para autoridade superior.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en-us">A decision-making step with accompanying decision logic used to determine execution approach for a process or to ensure that a process complies with governance criteria. For example, a sign-off control on the purchase request processing process that checks whether the total value of the request is within the sign-off limits of the requester, or whether it needs escalating to higher authority.</rdfs:comment>
    <dc:description xml:lang="en-us">A decision-making step with accompanying decision logic used to determine execution approach for a process or to ensure that a process complies with governance criteria. For example, a sign-off control on the purchase request processing process that checks whether the tota{@en-us}</dc:description>
    <dc:description xml:lang="pt-br">Uma etapa de tomada de decisão com lógica de decisão acompanhante utilizada para determinar a abordagem de execução de um processo ou para garantir que um processo esteja em conformidade com critérios de governança. Por exemplo, um controle de aprovação no processo de processamento de solicitação de compra que verifica se o valor total do pedido está dentro dos limites de assinatura do solicitante ou se ele precisa escalar para autoridade superior.</dc:description>
    <rdfs:label xml:lang="en-us">Control</rdfs:label>
    <rdfs:label xml:lang="pt-br">Controle</rdfs:label>
    <dc:title xml:lang="pt-br">Controle</dc:title>
    <dc:title xml:lang="en-us">Control</dc:title>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/resolves">
    <rdfs:label xml:lang="en-us">resolves</rdfs:label>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/pmv/Process"/>
          <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="pt-br">resolve um</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/pmv/Event"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://purl.org/pmv/is_resolved_by"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/supports">
    <rdfs:label xml:lang="pt-br">suporta</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Function"/>
    <rdfs:label xml:lang="en-us">supports</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/pmv/Process"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/is_resolved_by">
    <rdfs:label xml:lang="en-us">is resolved by</rdfs:label>
    <rdfs:label xml:lang="pt-br">é resolvido por</rdfs:label>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/pmv/Event"/>
          <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://purl.org/pmv/Process"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/is_produced_by">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://purl.org/pmv/produces"/>
    </owl:inverseOf>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/pmv/Process"/>
          <owl:Class rdf:about="http://www.w3.org/ns/org#Organization"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:label xml:lang="en-us">Is produced by</rdfs:label>
    <rdfs:label xml:lang="pt-br">é produzido por</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Product"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/participates_in">
    <rdfs:range rdf:resource="http://purl.org/pmv/Process"/>
    <rdfs:label xml:lang="en-us">participates in</rdfs:label>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:label xml:lang="pt-br">participa em</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/produces">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/pmv/Process"/>
          <owl:Class rdf:about="http://www.w3.org/ns/org#Organization"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="pt-br">produz</rdfs:label>
    <rdfs:label xml:lang="en-us">produces</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/pmv/Product"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/is_guided_by">
    <rdfs:label xml:lang="en-us">Is guided by</rdfs:label>
    <rdfs:label xml:lang="pt-br">é guiado por</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Process"/>
    <rdfs:range rdf:resource="http://purl.org/pmv/Control"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/owns">
    <rdfs:label xml:lang="pt-br">tem função</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/ns/org#Organization"/>
    <rdfs:label xml:lang="en-us">owns</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/pmv/Function"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/orchestrates">
    <rdfs:label xml:lang="pt-br">orquestra</rdfs:label>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="http://purl.org/pmv/Business_Service"/>
          <owl:Class rdf:about="http://purl.org/pmv/Function"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:label xml:lang="en-us">orchestrates</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Process"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/involves">
    <rdfs:label xml:lang="en-us">involves</rdfs:label>
    <rdfs:label xml:lang="pt-br">envolve</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <owl:inverseOf rdf:resource="http://purl.org/pmv/participates_in"/>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Process"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/pmv/ensures_correct_operation_of">
    <owl:inverseOf rdf:resource="http://purl.org/pmv/is_guided_by"/>
    <rdfs:label xml:lang="en-us">ensures correct operation of</rdfs:label>
    <rdfs:label xml:lang="pt-br">assegura a funcionamento de</rdfs:label>
    <rdfs:range rdf:resource="http://purl.org/pmv/Process"/>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Control"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/pmv/process_criticality">
    <rdfs:label xml:lang="en-us">process criticality</rdfs:label>
    <rdfs:comment xml:lang="en-us">criticality of this process to business operations.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="http://purl.org/pmv/Process"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/pmv/headcount">
    <rdfs:domain rdf:resource="http://www.w3.org/ns/org#Organization"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment xml:lang="en-us">Number of FTEs working within the organization.</rdfs:comment>
    <rdfs:label xml:lang="en-us">Headcount</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/pmv/FTE">
    <rdfs:label xml:lang="en-us">FTE</rdfs:label>
    <rdfs:comment xml:lang="en-us">Estimated number of FTEs that operate in this Role</rdfs:comment>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
</rdf:RDF>

